      <Param name="random seed" updates="None" val="" valType="code"/>
    </LoopInitiator>
    <Routine name="trial"/>
    <LoopTerminator name="trials"/>
    <Routine name="rest_run"/>
    <LoopTerminator name="run_loop"/>
    <Routine name="end"/>
  </Flow>
</PsychoPy2experiment>
