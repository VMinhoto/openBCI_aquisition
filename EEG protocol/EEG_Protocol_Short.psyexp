      <Param name="name" updates="None" val="run_loop" valType="code"/>
      <Param name="random seed" updates="None" val="" valType="code"/>
    </LoopInitiator>
    <LoopInitiator loopType="TrialHandler" name="trials">
      <Param name="Selected rows" updates="None" val="" valType="str"/>
      <Param name="conditions" updates="None" val="[OrderedDict([('text', 'Left Hand')]), OrderedDict([('text', 'Right Hand')]), OrderedDict([('text', 'Both Hands')]), OrderedDict([('text', 'Both Feet')])]" valType="str"/>
